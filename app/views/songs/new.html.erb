<h1>New Song Form</h1>

<%= form_tag songs_path, method: "post" do %>

  <div class="field">
    <%= label_tag "title", "Title" %>
    <%= text_field_tag "song[title]" %>
  </div>

  <div class="field">
    <%= label_tag "release_year", "Release Year" %>
    <%= text_field_tag "song[release_year]" %>
  </div>

  <div class="field">
    <%= label_tag "title", "Title" %>
    <%= text_field_tag "song[title]" %>
  </div>

<% end %>
